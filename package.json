{
  "name": "honey-do-list",
  "version": "0.1.0",
  "private": true,
  "description": "House chores get done with this app!",
  "main": "server/server.js",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.2.1"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deannapi/honey-do-list.git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "DeAnna Martinez, Christopher Short, Kiran Riaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deannapi/honey-do-list/issues"
  },
  "homepage": "https://github.com/deannapi/honey-do-list#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
